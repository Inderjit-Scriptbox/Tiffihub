{
    "expo": {
        "name": "TiffinHubProj",
        "slug": "TiffinHubProj",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./app/assets/basic/logo1.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./app/assets/logo/logo_color.png",
            "resizeMode": "contain",
            "color": "#FFFFFF"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./app/assets/basic/adaptive-icon.png",
                "backgroundColor": "#FFFFFF"
            }
        },
        "web": {
            "favicon": "./app/assets/basic/favicon.png"
        }
    }
}